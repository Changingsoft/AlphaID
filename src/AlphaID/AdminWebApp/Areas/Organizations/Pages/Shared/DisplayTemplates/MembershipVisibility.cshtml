@model MembershipVisibility
@switch (Model)
{
    case MembershipVisibility.Private:
        <span>@localizer["Private"]</span>
        break;
    case MembershipVisibility.AuthenticatedUser:
        <span>@localizer["AuthenticatedUser"]</span>
        break;
    case MembershipVisibility.Public:
        <span>@localizer["Public"]</span>
        break;
}