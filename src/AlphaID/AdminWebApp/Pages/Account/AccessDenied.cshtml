@page
@model AccessDeniedModel
@{
    Layout = "Tabler/_Page";
    ViewBag.Title = L["Access denied"];
}

<div class="container container-tight py-4">
    <div class="card">
        <div class="card-body">
            <h1>@ViewBag.Title</h1>
            <p>@L["You don't have permission to do this."]</p>
            <p>@L["Go back to home, or sign out your account."]</p>
            <a asp-page="/Index" asp-area="" class="btn">@L["Go home"]</a>
            <a asp-page="SignOut" class="btn">@L["Sign out"]</a>
        </div>
    </div>
</div>
