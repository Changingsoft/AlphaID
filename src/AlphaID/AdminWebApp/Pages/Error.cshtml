@page
@model ErrorModel
@{
    this.ViewBag.Title = "Error!";
}

<h1 class="text-danger">Error！:-(</h1>
<h2 class="text-danger">Errors occured when process requests.</h2>

@if (Model.ShowRequestId)
{
    <p>
        <strong>Request ID:</strong> <code>@Model.RequestId</code>
    </p>
}

<p>Contact administrator, or retry again later.</p>