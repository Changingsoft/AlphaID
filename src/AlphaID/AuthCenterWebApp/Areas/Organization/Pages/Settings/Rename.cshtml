@page
@model RenameModel
@{
    ViewBag.Title = L["Rename organization"];
}
<h2>@L["Rename organization"]</h2>
<form method="post">
    <div class="mb-3">
        <label asp-for="Name" class="form-label"></label>
        <input asp-for="Name" class="form-control"/>
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <button type="submit" class="btn">@L["Change name"]</button>
</form>

@section Scripts{
    <partial name="_ValidationScriptsPartial"/>
}