@model GenericOrganization

<h2>@Model.Name</h2>
<dl class="row">
    <dt class="col-4">统一社会信用代码</dt>
    <dd class="col-8">@Model.USCI</dd>
    <dt class="col-4">住所</dt>
    <dd class="col-8">@Model.Domicile</dd>
    <dt class="col-4">负责人/代表人</dt>
    <dd class="col-8">@Model.LegalPersonName</dd>
    <dt class="col-4">电话</dt>
    <dd class="col-8">@Model.Contact</dd>
</dl>