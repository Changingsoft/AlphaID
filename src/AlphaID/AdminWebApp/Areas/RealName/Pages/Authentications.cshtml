@page
@model AdminWebApp.Areas.RealName.Pages.AuthenticationsModel
@{
    ViewBag.Title = L["Real-name Authentications"];
}
<h1>@L["Real-name Authentications"]</h1>

<table class="table">
    <thead>
    <tr>
        <td>For</td>
        <td>Validated by</td>
        <td>Validated at</td>
        <td>Expires</td>
    </tr>
    </thead>
    <tbody>
    @foreach (var authentication in Model.Authentications)
    {
        <partial name="_RealNameAuthenticationRow" model="authentication"/>
    }
    </tbody>
</table>