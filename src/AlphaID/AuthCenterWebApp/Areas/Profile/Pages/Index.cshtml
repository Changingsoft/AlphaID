@page
@model IndexModel
@{
    this.ViewData["Title"] = "个人资料";
}



<h2>Claims</h2>
<table class="table">
    <thead>
        <tr>
            <td>Type</td>
            <td>Value</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in this.User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </tbody>
</table>
