@page
@model WhoModel
@{
}

<h1>There are two organizations has the same name. Who you want to see?</h1>

<table class="table">
    <thead>
    <tr>
        <th>Name</th>
        <th>Domicile</th>
        <th>Representative</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var org in Model.Organizations)
    {
        <tr>
            <td>
                <a asp-page="Index" asp-route-anchor="@org.Id">@org.Name</a>
            </td>
            <td>@org.Domicile</td>
            <td>@org.Representative</td>
        </tr>
    }
    </tbody>
</table>