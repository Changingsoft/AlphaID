@{
    Layout = "/Pages/Shared/_Layout.cshtml";
}
@section Heads{
    @RenderSection("Heads", false)
}

<div class="page-header">
    <div class="container-xl">
        <ul class="breadcrumb">
            <li class="breadcrumb-item"><a asp-page="/Index">Organizations</a></li>
            @await RenderSectionAsync("Breadcrumb", false)
        </ul>
        @await RenderSectionAsync("PageHeader", false)
    </div>
</div>
<div class="page-body">
    <div class="container-xl">
        @RenderBody()
    </div>
</div>

@section Scripts{
    @RenderSection("Scripts", false)
}
